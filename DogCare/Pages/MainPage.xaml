<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="DogCare.MainPage"
             NavigationPage.HasBackButton="False"
             NavigationPage.HasNavigationBar="True">
    <StackLayout>
        <Button Text="My dog calendar" Clicked="Calendar_Clicked"/>
        <Button Text="Go on a walk" Clicked="Walk_Clicked"/>
        <Button Text="Competition" Clicked="Competition_Clicked"/>
        <Button Text="My walks" Clicked="MyWalks_Clicked"/>
    </StackLayout>
</ContentPage>