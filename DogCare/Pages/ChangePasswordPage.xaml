<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="DogCare.ChangePasswordPage">
    <StackLayout>
        
        <Entry x:Name="old" Placeholder="Old Password"/>
        <Entry x:Name="newP" Placeholder="New Password"/>
        <Entry x:Name="confirm" Placeholder="Confirm password"/>
        <Button Text="Change" Clicked="ChangePassword_Clicked"/>
    </StackLayout>
</ContentPage>